    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "rimraf": "^3.0.2",
    "testdouble": "^3.16.1",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^26.4.1",
